{
  "name": "fullstack-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/ts/**/*.ts' 'src/ts/**/*.tsx'",
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "dev-client": "webpack-dev-server --env.platform=web --config config/webpack.config.js/base.js -w --debug --output-path-info",
    "dev-client:hot": "webpack-dev-server -w --hot",
    "build-hx-server": "haxe build-server.hxml && touch finished-building-hx-server",
    "build-hx-client": "haxe build-client.hxml && touch finished-building-hx-client",
    "build-js-server": "webpack --env.platform=server --config config/webpack.config.js/base.js && touch finished-building-js-server",
    "dev-server": "nodemon --verbose -e ts,js,hx --ignore src/ts/client/client.js --ignore src/ts/client/hx.js --ignore src/ts/client/hx.d.ts --ignore src/ts/server/hx.js --ignore src/ts/server/hx.d.ts --watch ./src/ts --watch src/hx --exec \"npm run build-js-server && concurrently 'npm run build-hx-server' 'npm run build-hx-client' && wait-on finished-building-hx-server finished-building-hx-client && rm finished-building-hx-client finished-building-hx-server finished-building-js-server && node index.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "dev:hot": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client:hot\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "nodemonConfig": {
    "delay": "1000"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "author": "Marcelo Serpa",
  "jest": {
    "roots": [
      "<rootDir>/src/ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/ts/client/setupEnzyme.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/src/ts/config-for-tests.json"
      }
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.2.5",
    "@material-ui/icons": "3.0.2",
    "body-parser": "^1.18.3",
    "chance": "^1.0.18",
    "class-autobind": "^0.1.4",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-manifest-helpers": "^0.6.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.2",
    "nodemon": "^1.19.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@material-ui/core": "3.9.3",
    "@types/enzyme": "^3.1.15",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.3",
    "concurrently": "^4.1.0",
    "core-js": "^3.2.1",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "haxe-loader": "^0.10.0",
    "http-proxy-middleware": "^0.19.1",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.11.0",
    "npm-watch": "^0.6.0",
    "open-browser-webpack-plugin": "github:rodrigopandini/open-browser-webpack-plugin",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.3",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^23.10.5",
    "tsc-watch": "^1.0.31",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typeface-roboto": "^0.0.54",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "wait-on": "^3.3.0",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
